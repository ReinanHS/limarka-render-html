{
  "name": "reinanhs/limarka-render-html",
  "type": "project",
  "description": "Responsible for rendering a web page to limarka",
  "keywords": [
    "limarka",
    "html",
    "render"
  ],
  "license": "MIT",
  "require": {
    "tightenco/jigsaw": "^1.3",
    "ext-yaml": "*",
    "php": "^8.1",
    "ext-fileinfo": "*"
  },
  "autoload": {
    "classmap": [
      "app.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
